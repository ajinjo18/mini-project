<!-- Product Section Begin -->
<section class="product spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-4">
                <div class="section-title">
                    <h4>New product</h4>
                </div>
            </div>
            <div class="col-lg-8 col-md-8">
                <ul class="filter__controls">
                    <li class="active" data-filter="*">All</li>
                    <li data-filter=".<%= bannerc[0].category %>"><%= bannerc[0].category %>’s</li>
                    <li data-filter=".<%= bannerc[1].category %>"><%= bannerc[1].category %>’s</li>
                    <li data-filter=".<%= bannerc[2].category %>"><%= bannerc[2].category %>’s</li>
