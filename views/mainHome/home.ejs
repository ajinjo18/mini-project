<!-- Discount Section Begin -->
<section style="margin-top: 50px;" class="discount">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 p-0">
                <div class="discount__pic">
                    <img src="/img/discount.jpg" alt="">
                </div>
            </div>
            <div class="col-lg-6 p-0">
                <div class="discount__text">
                    <div class="discount__text__title">
                        <h2><%= offerbaner[0].sale %></h2>
                        <h5><span>Sale</span> <%= offerbaner[0].discount %></h5>
                    </div>
                    <div class="discount__countdown" id="countdown-time">
                        <div class="countdown__item">
                            <span><%= offerbaner[0].day %></span>
                            <p>Days</p>
                        </div>
                        <div class="countdown__item">
                            <span><%= offerbaner[0].hour %></span>
                            <p>Hour</p>
                        </div>
                        <div class="countdown__item">
                            <span><%= offerbaner[0].min %></span>
                            <p>Min</p>
                        </div>
                        <div class="countdown__item">
                            <span><%= offerbaner[0].sec %></span>
                            <p>Sec</p>
                        </div>
                    </div>
                    <a href="/home/shop">Shop now</a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Discount Section End -->

